{
    "name": "hifny/laravel-couchbase",
    "description": "A Couchbase based Eloquent model and Query builder for Laravel",
    "keywords": ["laravel","eloquent","couchbase","database","model"],
    "homepage": "https://github.com/ORT-Interactive-GmbH/laravel-couchbase",
    "authors": [
    ],
    "license" : "MIT",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "orchestra/testbench": "^3.4.0",
        "mockery/mockery": "^0.9"
    },
    "autoload": {
        "psr-4": {
            "Hifny\\Couchbase\\": "src/",
            "Hifny\\Eloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models",
            "tests/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hifny\\Couchbase\\CouchbaseServiceProvider"
            ]
        }
    }
}
